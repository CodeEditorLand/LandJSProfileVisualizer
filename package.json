{
	"name": "js-profile-visualizer",
	"version": "0.0.1",
	"private": false,
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandJSProfileVisualizer#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandJSProfileVisualizer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandJSProfileVisualizer.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"workspaces": [
		"packages/vscode-js-profile-core",
		"packages/vscode-js-profile-flame",
		"packages/vscode-js-profile-table"
	],
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"clean": "rimraf -g \"packages/*/out\"",
		"compile": "npm run clean && npm run compile:core && concurrently \"npm:compile:table\" \"npm:compile:flame\"",
		"compile:core": "npm run -w vscode-js-profile-core compile",
		"compile:flame": "npm run -w vscode-js-profile-flame compile",
		"compile:table": "npm run -w vscode-js-profile-table compile",
		"pack": "npm run compile && npm run -ws pack",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"test:unit": "vitest run",
		"watch:core": "npm run -w vscode-js-profile-core watch",
		"watch:flame": "npm run -w vscode-js-profile-flame watch",
		"watch:table": "npm run -w vscode-js-profile-table watch"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "8.1.0",
		"@types/node": "20.12.7",
		"autoprefixer": "10.4.19",
		"chokidar-cli": "3.0.0",
		"concurrently": "8.2.2",
		"cpy-cli": "5.0.0",
		"cross-env": "7.0.3",
		"css-loader": "7.1.1",
		"cssnano": "7.0.1",
		"devtools-protocol": "0.0.1294763",
		"glob": "10.3.12",
		"os-browserify": "0.3.0",
		"path-browserify": "1.0.1",
		"postcss-loader": "8.1.1",
		"raw-loader": "4.0.2",
		"rimraf": "5.0.5",
		"source-map-loader": "5.0.0",
		"source-map-support": "0.5.21",
		"style-loader": "4.0.0",
		"svg-inline-loader": "0.8.2",
		"ts-loader": "9.5.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
