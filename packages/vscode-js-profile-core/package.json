{
  "name": "vscode-js-profile-core",
  "version": "0.0.1",
  "private": "false",
  "sideEffects": false,
  "scripts": {
    "compile": "concurrently \"npm:compile:*\"",
    "compile:css": "cpy \"src/**/*.css\" out/esm",
    "compile:esm": "tsc -p tsconfig.browser.json",
    "compile:ext": "tsc -p tsconfig.json",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:css": "npm run compile:css && chokidar \"src/**/*.css\" -c \"cpy --parents --cwd=src '**/*.css' ../out/esm\"",
    "watch:esm": "tsc --watch -p tsconfig.browser.json",
    "watch:ext": "tsc --watch",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "dependencies": {
    "@vscode/codicons": "0.0.35",
    "@vscode/v8-heap-parser": "0.1.0"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "type": "module",
  "publisher": "playform",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "devDependencies": {
    "typescript-esbuild": "0.3.1"
  }
}
